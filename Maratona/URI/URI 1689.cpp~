#include <cstdio>
#include <iostream>
#include <string.h>
#include <vector>

using namespace std;

int t, n, k;
vector<int> pos, value;

int main(){
  scanf("%d", &t);
  for(int q = 0; q < t; q++){
    pos.clear; value.clear();
    scanf("%d %d", &n, &k);
    for(int i = 0; i < n; i++){
      int x; cin >> x;
      pos.push_back(x);
    }
    for(int i = 0; i < n; i++){
      int x; cin >> x;
      value.push_back(x);
    }
  }
  return 0;
}
